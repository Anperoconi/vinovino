{
  "name": "frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve -o",
    "build": "ng build",
    "admin": "ng serve admin -o",
    "build:stag": "ng build --configuration=stagging && cp ../backend/public/assets/angular/index.html ../backend/resources/views/angular.blade.php",
    "build:prod": "ng build --configuration=production && cp ../backend/public/assets/angular/index.html ../backend/resources/views/angular.blade.php",
    "build-admin:dev": "ng build admin --configuration=local && cp ../backend/public/assets/admin/index.html ../backend/resources/views/admin.blade.php",
    "build-admin:stag": "ng build admin --configuration=stagging && cp ../backend/public/assets/admin/index.html ../backend/resources/views/admin.blade.php",
    "build-admin:prod": "ng build admin --configuration=production && cp ../backend/public/assets/admin/index.html ../backend/resources/views/admin.blade.php",
    "build:staging": "ng build --configuration=staging && cp ../backend/public/assets/angular/index.html ../backend/resources/views/angular.blade.php",
    "watch": "ng build --watch --configuration dev",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.3",
    "@angular/cdk": "^15.2.5",
    "@angular/common": "^15.2.0",
    "@angular/compiler": "^15.2.0",
    "@angular/core": "^15.2.0",
    "@angular/forms": "^15.2.0",
    "@angular/material": "^15.2.3",
    "@angular/platform-browser": "^15.2.0",
    "@angular/platform-browser-dynamic": "^15.2.0",
    "@angular/router": "^15.2.0",
    "@angular/service-worker": "^15.2.0",
    "angular-highcharts": "^15.0.1",
    "highcharts": "^10.3.3",
    "ngx-scanner-qrcode": "^1.2.7",
    "quagga": "^0.12.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.1",
    "@angular/cli": "~15.2.1",
    "@angular/compiler-cli": "^15.2.0",
    "@types/jasmine": "~4.3.0",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "typescript": "~4.9.4"
  }
}
